	<sql id="Base_Column_List"> loan_info_id, loan_cate_id, state, app_name,
		vice_app_name,
		max_money, applicants_num, day_repayment, all_rota,
		day_interest,
		loan_range_start, loan_range_end, time_limit_start,
		time_limit_end,
		link_url_h, app_con, audit_desc, product_intr,
		image_url,
		apply_process, is_recommend, prof_identity, money_fastest,
		rate_min,
		max_success, sort, create_time, create_man, modify_time,
		modify_man,
		remark,times
	</sql>

	<select id="get" parameterType="map" resultMap="LoanInfoResultMap">
		select
		<include refid="Base_Column_List" />
		from loan_info
	</select>

	<select id="queryCount" parameterType="map" resultType="int">
		select count(loan_info_id) from loan_info where 1=1
		<if test="appName !=null"> and app_name like concat('%',#{appName},'%')</if>
		<if test="state !=null"> and state = #{state}</if>
		<if test="isRecommend !=null"> and is_recommend = #{isRecommend}</if>
		<if test="loanCateId !=null"> and loan_cate_id = #{loanCateId}</if>
	</select>
	<select id="query" parameterType="map" resultMap="LoanInfoResultMap">
		select
		<include refid="Base_Column_List" />
		from loan_info where 1=1
		<if test="appName !=null"> and app_name like concat('%',#{appName},'%')</if>
		<if test="state !=null"> and state = #{state}</if>
		<if test="isRecommend !=null"> and is_recommend = #{isRecommend}</if>
		<if test="loanCateId !=null"> and loan_cate_id = #{loanCateId}</if>
		limit ${iDisplayStart},${iDisplayLength}
	</select>

	<select id="queryList" resultMap="LoanInfoResultMap">
		select
		<include refid="Base_Column_List" />
		from loan_info where 1=1
	</select>

	<insert id="insert" parameterType="map">
		INSERT INTO loan_info
		(loan_info_id,loan_cate_id,state,app_name,vice_app_name,max_money,applicants_num,day_repayment,all_rota,day_interest,loan_range_start,loan_range_end,time_limit_start,time_limit_end,link_url_h,app_con,audit_desc,product_intr,image_url,apply_process,is_recommend,prof_identity,money_fastest,rate_min,max_success,sort,create_time,create_man,modify_time,modify_man,remark,times)
		VALUES
		(#{loanInfoId},#{loanCateId},#{state},#{appName},#{viceAppName},#{maxMoney},#{applicantsNum},#{dayRepayment},#{allRota},#{dayInterest},#{loanRangeStart},#{loanRangeEnd},#{timeLimitStart},#{timeLimitEnd},#{linkUrlH},#{appCon},#{auditDesc},#{productIntr},#{imageUrl},#{applyProcess},#{isRecommend},#{profIdentity},#{moneyFastest},#{rateMin},#{maxSuccess},#{sort},#{createTime},#{createMan},#{modifyTime},#{modifyMan},#{remark},#{times})
	</insert>

	<delete id="delete" parameterType="map">
		DELETE FROM loan_info WHERE
		loan_info_id = #{arg0}
	</delete>

	<select id="getLoanInfoById" parameterType="map" resultMap="LoanInfoResultMap">
		SELECT
		<include refid="Base_Column_List" />
		FROM loan_info WHERE loan_info_id = #{arg0}
	</select>

	<update id="update" parameterType="map">
		UPDATE loan_info
		<set>

			<if test="loanCateId != null"> loan_cate_id = #{loanCateId}, </if>

			<if test="state != null"> state = #{state}, </if>

			<if test="appName != null"> app_name = #{appName}, </if>

			<if test="viceAppName != null"> vice_app_name = #{viceAppName}, </if>

			<if test="maxMoney != null"> max_money = #{maxMoney}, </if>

			<if test="applicantsNum != null"> applicants_num = #{applicantsNum}, </if>

			<if test="dayRepayment != null"> day_repayment = #{dayRepayment}, </if>

			<if test="allRota != null"> all_rota = #{allRota}, </if>

			<if test="dayInterest != null"> day_interest = #{dayInterest}, </if>

			<if test="loanRangeStart != null"> loan_range_start = #{loanRangeStart}, </if>

			<if test="loanRangeEnd != null"> loan_range_end = #{loanRangeEnd}, </if>

			<if test="timeLimitStart != null"> time_limit_start = #{timeLimitStart}, </if>

			<if test="timeLimitEnd != null"> time_limit_end = #{timeLimitEnd}, </if>

			<if test="linkUrlH != null"> link_url_h = #{linkUrlH}, </if>

			<if test="appCon != null"> app_con = #{appCon}, </if>

			<if test="auditDesc != null"> audit_desc = #{auditDesc}, </if>

			<if test="productIntr != null"> product_intr = #{productIntr}, </if>

			<if test="imageUrl != null"> image_url = #{imageUrl}, </if>

			<if test="applyProcess != null"> apply_process = #{applyProcess}, </if>

			<if test="isRecommend != null"> is_recommend = #{isRecommend}, </if>

			<if test="profIdentity != null"> prof_identity = #{profIdentity}, </if>

			<if test="moneyFastest != null"> money_fastest = #{moneyFastest}, </if>

			<if test="rateMin != null"> rate_min = #{rateMin}, </if>

			<if test="maxSuccess != null"> max_success = #{maxSuccess}, </if>

			<if test="sort != null"> sort = #{sort}, </if>

			<if test="createTime != null"> create_time = #{createTime}, </if>

			<if test="createMan != null"> create_man = #{createMan}, </if>

			<if test="modifyTime != null"> modify_time = #{modifyTime}, </if>

			<if test="modifyMan != null"> modify_man = #{modifyMan}, </if>

			<if test="remark != null"> remark = #{remark}, </if>
			
			<if test="times != null"> times = #{times}, </if>

		</set>
		WHERE loan_info_id = #{loanInfoId}
	</update>




</mapper>
