		<id property="dayInterest" column="day_interest" />
		<id property="loanRangeStart" column="loan_range_start" />
		<id property="loanRangeEnd" column="loan_range_end" />
		<id property="timeLimitStart" column="time_limit_start" />
		<id property="timeLimitEnd" column="time_limit_end" />
		<id property="linkUrlH" column="link_url_h" />
