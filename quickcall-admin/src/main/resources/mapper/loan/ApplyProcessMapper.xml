<?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ApplyProcess">
	<resultMap id="ApplyProcessResultMap" type="ApplyProcess">
		<id property="applyProcessId" column="apply_process_id" />
		<id property="useId" column="use_id" />
		<id property="processName" column="process_name" />
		<id property="processImage" column="process_image" />
		<id property="createTime" column="create_time" />
		<id property="createMan" column="create_man" />
		<id property="modifyTime" column="modify_time" />
		<id property="modifyMan" column="modify_man" />
		<id property="remark" column="remark" />
		<id property="sort" column="sort" />
	</resultMap>

	<sql id="Base_Column_List">
		apply_process_id, use_id, process_name, process_image,
		create_time,
		create_man,
		modify_time, modify_man, remark,sort
	</sql>

	<select id="get" parameterType="map" resultMap="ApplyProcessResultMap">
		select
		<include refid="Base_Column_List" />
		from apply_process
	</select>
	
	<select id="queryGroup" resultMap="ApplyProcessResultMap">
		select
		<include refid="Base_Column_List" />
		from apply_process group by process_name
	</select>

	<select id="queryCount" parameterType="map" resultType="int">
		select count(apply_process_id) from apply_process where 1=1
		<if test="processName !=null"> and process_name like concat('%',#{processName},'%')</if>
	</select>
	<select id="query" parameterType="map" resultMap="ApplyProcessResultMap">
		select
		<include refid="Base_Column_List" />
		from apply_process where 1=1
		<if test="processName !=null"> and process_name like concat('%',#{processName},'%')</if>
		limit ${iDisplayStart},${iDisplayLength}
	</select>

	<insert id="insert" parameterType="map">
		INSERT INTO apply_process
		(apply_process_id,use_id,process_name,process_image,create_time,create_man,modify_time,modify_man,remark,sort)
		VALUES
		(#{applyProcessId},#{useId},#{processName},#{processImage},#{createTime},#{createMan},#{modifyTime},#{modifyMan},#{remark},#{sort})
	</insert>

	<delete id="delete" parameterType="map">
		DELETE FROM apply_process
		WHERE apply_process_id = #{arg0}
	</delete>

	<select id="getApplyProcessById" parameterType="map"
		resultMap="ApplyProcessResultMap">
		SELECT
		<include refid="Base_Column_List" />
		FROM apply_process
		WHERE apply_process_id = #{arg0}
	</select>

	<update id="update" parameterType="map">
		UPDATE apply_process
		<set>

			<if test="useId != null"> use_id = #{useId}, </if>

			<if test="processName != null"> process_name = #{processName}, </if>

			<if test="processImage != null"> process_image = #{processImage}, </if>

			<if test="createTime != null"> create_time = #{createTime}, </if>

			<if test="createMan != null"> create_man = #{createMan}, </if>

			<if test="modifyTime != null"> modify_time = #{modifyTime}, </if>

			<if test="modifyMan != null"> modify_man = #{modifyMan}, </if>

			<if test="remark != null"> remark = #{remark}, </if>
			
			<if test="sort != null"> sort = #{sort}, </if>

		</set>
		WHERE apply_process_id = #{applyProcessId}
	</update>




</mapper>
