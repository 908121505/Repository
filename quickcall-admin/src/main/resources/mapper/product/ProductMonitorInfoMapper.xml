	<!-- 查询（根据主键条件查询） -->
	<select id="query" resultMap="BaseResultMap" parameterType="map">
		SELECT
		<include refid="Base_Column_List" />
		FROM product_monitor_info
		where 1=1
			<if test="productId != null">
				and product_id = #{productId}
			</if>
			<if test="productName != null">
				and product_name = #{productName}
			</if>
			<if test="url != null">
				and url = #{url}
			</if>
			<if test="key != null">
				and `key` = #{key}
			</if>
			<if test="valueM != null">
				AND `value` = #{valueM}
			</if>
			<if test="state != null">
				AND state = #{state}
			</if>
			<if test="createTime != null">
				AND create_time = #{createTime}
			</if>
			<if test="createMan != null">
				AND create_man = #{createMan
			</if>
			<if test="modifyTime != null">
				AND modify_time = #{modifyTime}
			</if>
			<if test="modifyMan != null">
				AND modify_man = #{modifyMan}
			</if>
			<if test="remark != null">
				AND remark = #{remark}
			</if>
		order by create_time desc
		<if test="iDisplayStart!=null and iDisplayLength!=''">
			limit ${iDisplayStart},${iDisplayLength}
		</if>
	</select>
	<!-- 统计（根据主键条件查询） -->
	<select id="queryMonitorInfoCount" resultType="int" parameterType="map">
		SELECT
		count(1)
		FROM product_monitor_info
		where 1 = 1
			<if test="productId != null">
				and product_id = #{productId}
			</if>
			<if test="productName != null">
				and product_name = #{productName}
			</if>
			<if test="url != null">
				and url = #{url}
			</if>
			<if test="key != null">
				and `key` = #{key}
			</if>
			<if test="value != null">
				and `value` = #{value}
			</if>
			<if test="state != null">
				and state = #{state}
			</if>
			<if test="createTime != null">
				and create_time = #{createTime}
			</if>
			<if test="createMan != null">
				and create_man = #{createMan}
			</if>
			<if test="modifyTime != null">
				and modify_time = #{modifyTime}
			</if>
			<if test="modifyMan != null">
				and modify_man = #{modifyMan}
			</if>
			<if test="remark != null">
				and remark = #{remark}
			</if>
	</select>

	<!--通过产品名称查询产品ID-->
	<select id="queryProductId" resultType="map" parameterType="map">
		SELECT t.product_id
		FROM product t
		WHERE t.product_name = #{productName}
	</select>

