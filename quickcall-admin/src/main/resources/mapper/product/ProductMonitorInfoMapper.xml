	<!-- 通用查询结果列-->
	<sql id="Base_Column_List">
product_monitor_info_id,	product_id,	product_name,	url,	`key`,	`value`,	state,	create_time,	create_man,	modify_time,	modify_man,	remark
	</sql>

	<!-- 查询（根据主键ID查询） -->
	<select id="get" resultMap="BaseResultMap" parameterType="map">
		 SELECT
		 <include refid="Base_Column_List" />
		 FROM product_monitor_info
		 WHERE product_monitor_info_id = #{arg0}
	</select>

	<!-- 查询（根据主键ID查询） -->
	<select id="findAll" resultMap="BaseResultMap" parameterType="java.lang.String">
