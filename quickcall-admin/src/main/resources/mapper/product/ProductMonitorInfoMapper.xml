		 WHERE product_monitor_info_id = #{arg0}
	</delete>

	<!-- 添加 -->
	<insert id="insert" parameterType="com.calf.module.product.entity.ProductMonitorInfo">
		 INSERT INTO product_monitor_info
 		(product_monitor_info_id,product_id,product_name,url,`key`,`value`,state,create_time,create_man,modify_time,modify_man,remark)
		 VALUES 
 		(#{productMonitorInfoId},#{productId},#{productName},#{url},#{key},#{valueM},#{state},#{createTime},#{createMan},#{modifyTime},#{modifyMan},#{remark})
	</insert>

	<!-- 修 改-->
	<update id="update" parameterType="map">
		 UPDATE product_monitor_info
 		 <set> 
			<if test="productId != null">
				 product_id = #{productId},
			</if>
			<if test="productName != null">
				 product_name = #{productName},
			</if>
			<if test="url != null">
				 url = #{url},
			</if>
			<if test="key != null">
				 `key` = #{key},
			</if>
			<if test="valueM != null">
				 `value` = #{valueM},
			</if>
			<if test="state != null">
				 state = #{state},
			</if>
			<if test="createTime != null">
				 create_time = #{createTime},
			</if>
			<if test="createMan != null">
