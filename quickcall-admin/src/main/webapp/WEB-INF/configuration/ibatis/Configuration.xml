<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration     PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <typeAliases>
        <!-- common -->
        <typeAlias type="com.calf.module.common.entity.Ztree" alias="ZTree"/>
        <typeAlias type="com.calf.module.common.entity.Combobox" alias="Combobox"/>

        <!-- home -->
        <typeAlias type="com.calf.module.system.entity.QueryHomeMenu" alias="QueryHomeMenu"/>
        <typeAlias type="com.calf.module.system.entity.QueryHomeClick" alias="QueryHomeClick"/>
        <typeAlias type="com.calf.module.system.entity.QueryHomeAllClick" alias="QueryHomeAllClick"/>

        <!-- system -->
        <typeAlias type="com.calf.module.system.entity.SysUser" alias="SysUser"/>
        <typeAlias type="com.calf.module.system.entity.SysUserRole" alias="SysUserRole"/>
        <typeAlias type="com.calf.module.system.entity.SysRole" alias="SysRole"/>
        <typeAlias type="com.calf.module.system.entity.SysMenu" alias="SysMenu"/>
        <typeAlias type="com.calf.module.system.entity.SysRoleMenu" alias="SysRoleMenu"/>
        <typeAlias type="com.calf.module.system.entity.SysLog" alias="SysLog"/>
        <typeAlias type="com.calf.module.system.entity.QueryOperUserRole" alias="QueryOperUserRole"/>
        <typeAlias type="com.calf.module.system.entity.BizParam" alias="BizParam"/>
        <typeAlias type="com.calf.module.system.entity.BizParamType" alias="BizParamType"/>

        <!-- export -->
        <typeAlias type="com.calf.module.export.entity.ExportPlayData" alias="ExportPlayData"/>
        <typeAlias type="com.calf.module.export.entity.ExportOrderData" alias="ExportOrderData"/>
        <typeAlias type="com.calf.module.export.entity.ExportUserData" alias="ExportUserData"/>

        <!-- APP配置 -->
        <typeAlias type="com.calf.module.appconfig.entity.Banner" alias="Banner"/>

    </typeAliases>
</configuration>