    <dubbo:application name="${spring.dubbo.application.name}"/>
    <!-- 用dubbo协议在30001 -->
    <dubbo:protocol name="dubbo" port="${spring.dubbo.protocol.port}" />
    <!-- 注册中心的ip地址 -->
    <dubbo:registry address="${spring.dubbo.registry.address}"/>
    <!-- 扫描注解包路径，多个包用逗号分隔，不填pacakge表示扫描当前ApplicationContext中所有的类 -->
    <dubbo:annotation package="${spring.dubbo.scan}"/>

</beans>