<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://code.alibabatech.com/schema/dubbo
    http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:application name="quickcall-account-client" owner="len.song" organization="xiaoniu"/>

    <!-- 注册服务端 -->
    <dubbo:reference group="accountCenter" id="paymentDubboBusiness" interface="com.honglu.quickcall.account.facade.business.PaymentDubboBusiness" check="false" version="0.0.1"/>

    <dubbo:reference group="accountCenter" id="accountDubboBusiness" interface="com.honglu.quickcall.account.facade.business.AccountDubboBusiness" check="false" version="0.0.1"/>

    <dubbo:reference group="accountCenter" id="accountCommonBusiness" interface="com.honglu.quickcall.account.facade.business.AccountCommonBusiness" check="false" version="0.0.1"/>

</beans>