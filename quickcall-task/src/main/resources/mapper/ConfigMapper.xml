<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.honglu.quickcall.task.dao.ConfigMapper" >
	<select id="queryConfigById" parameterType="Integer" resultType="Config">
		SELECT
			C.ID,
			C.CONFIG_KEY,
			C.CONFIG_VALUE,
			C.CONFIG_POSTSCRIPT
		FROM
			CONFIG C
		WHERE
			C.ID = #{id}
	</select>
</mapper>